{
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "remoteUser": "vscode",
  "features": {
    "./auto-secrets-manager": {
      "secretManager": "infisical",
      "shells": "both",
      "branchMapping": "{}",
      "secretManagerConfig": "{}",
      "autoCommands": "{}",
      "cacheConfig": "{\"refresh_interval\":\"15m\",\"cleanup_interval\":\"7d\"}",
      "showEnvInPrompt": true,
      "markHistory": true,
      "debug": true,
      "branchDetection": true
    }
  }
}
